{
  "name": "keystroke_app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Comprehensive AI-powered typing test and analysis tool with a full featured dictionary and word search. Were deep elaborated dictionaries with a deep analysis of the words and their meanings.",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "dev:webpack": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "seed": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" prisma/seed.ts",
    "setup-db": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" prisma/setup-db.ts",
    "p-generate": "dotenv-run --env .env.local -- prisma generate",
    "p-migrate:deploy": "dotenv-run --env .env.local -- prisma migrate deploy",
    "p-migrate:dev": "dotenv-run --env .env.local -- prisma migrate dev",
    "p-studio": "dotenv-run --env .env.local -- prisma studio",
    "p-new-setup": "dotenv-run --env .env.local -- prisma migrate reset && rm -rf node_modules/.prisma && rm -rf node_modules/prisma && pnpm install && dotenv-run --env .env.local -- prisma migrate dev --name init && dotenv-run --env .env.local -- prisma generate",
    "p-change-schema": "dotenv-run --env .env.local -- prisma migrate dev && dotenv-run --env .env.local -- prisma migrate deploy && rm -rf node_modules/.prisma && rm -rf node_modules/@prisma && pnpm install @prisma/client && dotenv-run --env .env.local -- prisma generate",
    "prepare": "husky",
    "postinstall": "patch-package",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "validate-env": "node scripts/validate-env.mjs",
    "env:check": "node scripts/validate-env.mjs --check-files",
    "env:template": "node scripts/validate-env.mjs --template",
    "prebuild": "pnpm run validate-env",
    "analyze": "cross-env ANALYZE=true pnpm run build",
    "analyze:open": "cross-env ANALYZE=true pnpm run build && start .next/analyze/client.html",
    "analyze:server": "cross-env ANALYZE=true pnpm run build && start .next/analyze/nodejs.html",
    "docs:validate": "node scripts/validate-docs.mjs",
    "docs:check": "node scripts/validate-docs.mjs"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@auth/core": "^0.38.0",
    "@auth/prisma-adapter": "^2.9.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/react-use-effect-event": "^0.0.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@t3-oss/env-nextjs": "^0.13.4",
    "@tanstack/react-table": "^8.21.3",
    "@types/memory-cache": "^0.2.6",
    "@types/uuid": "^10.0.0",
    "@vercel/blob": "^0.27.3",
    "@vercel/postgres": "^0.10.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^4.3.16",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^6.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "extended-google-translate-api": "^2.0.2",
    "input-otp": "^1.4.2",
    "install": "^0.13.0",
    "lucide-react": "^0.487.0",
    "memory-cache": "^0.2.0",
    "next": "^15.3.2",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "pexels": "^1.4.0",
    "prisma": "^6.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.56.4",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^3.25.16"
  },
  "devDependencies": {
    "@dotenv-run/cli": "^1.3.6",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@next/bundle-analyzer": "^15.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.5.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "module-alias": "^2.2.3",
    "patch-package": "^8.0.0",
    "postcss": "^8.5.3",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.5.3",
    "prisma-generator-typescript-interfaces": "^2.1.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=20.12.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "bufferutil",
      "esbuild",
      "postinstall-postinstall",
      "prisma",
      "sharp"
    ],
    "overrides": {
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2"
    }
  }
}
